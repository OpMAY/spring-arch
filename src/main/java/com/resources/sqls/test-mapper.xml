<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.TestMapper">

    <select id="select" parameterType="String" resultType="String">
       SELECT * FROM User WHERE ID = #{id} AND ID IS NOT NULL
    </select>

    <insert id="registerUser" parameterType="User" >
        INSERT INTO USER VALUES (#{id},#{name},#{access_token},#{profile_img, typeHandler=JsonObjectTypeHandler},DEFAULT)
    </insert>



</mapper>